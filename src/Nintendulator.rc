IDR_NINTENDULATOR MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open...\tCtrl+O",            ID_FILE_OPEN
        MENUITEM "&Close",                      ID_FILE_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "Edit iNES &Header",           ID_FILE_HEADER
        MENUITEM SEPARATOR
        MENUITEM "&Auto-Run",                   ID_FILE_AUTORUN
        MENUITEM SEPARATOR
        MENUITEM "&Browse Save Files",          ID_FILE_BROWSESAVES
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tAlt+F4",               ID_FILE_EXIT
    END
    POPUP "&CPU"
    BEGIN
        MENUITEM "&Run\tF2",                    ID_CPU_RUN
        MENUITEM "&Step\tShift+F2",             ID_CPU_STEP
        MENUITEM "S&top\tF3",                   ID_CPU_STOP
        MENUITEM "So&ft Reset\tF4",             ID_CPU_SOFTRESET
        MENUITEM "H&ard Reset\tShift+F4",       ID_CPU_HARDRESET
        MENUITEM SEPARATOR
        MENUITEM "Sa&ve State\tF5",             ID_CPU_SAVESTATE
        MENUITEM "&Load State\tF8",             ID_CPU_LOADSTATE
        MENUITEM "&Prev State\tF6",             ID_CPU_PREVSTATE
        MENUITEM "&Next State\tF7",             ID_CPU_NEXTSTATE
        MENUITEM SEPARATOR
        MENUITEM "&Game Genie\tCtrl+G",         ID_CPU_GAMEGENIE
        MENUITEM "L&og Invalid Opcodes",        ID_CPU_BADOPS
        MENUITEM SEPARATOR
        POPUP "Fra&me Step"
        BEGIN
            MENUITEM "&Enabled\tCtrl+F",            ID_CPU_FRAMESTEP_ENABLED
            MENUITEM "&Step\tSpace",                ID_CPU_FRAMESTEP_STEP
        END
    END
    POPUP "&PPU"
    BEGIN
        POPUP "&Frameskip"
        BEGIN
            MENUITEM "&Auto",                       ID_PPU_FRAMESKIP_AUTO
            MENUITEM SEPARATOR
            MENUITEM "&0",                          ID_PPU_FRAMESKIP_0
            MENUITEM "&1",                          ID_PPU_FRAMESKIP_1
            MENUITEM "&2",                          ID_PPU_FRAMESKIP_2
            MENUITEM "&3",                          ID_PPU_FRAMESKIP_3
            MENUITEM "&4",                          ID_PPU_FRAMESKIP_4
            MENUITEM "&5",                          ID_PPU_FRAMESKIP_5
            MENUITEM "&6",                          ID_PPU_FRAMESKIP_6
            MENUITEM "&7",                          ID_PPU_FRAMESKIP_7
            MENUITEM "&8",                          ID_PPU_FRAMESKIP_8
            MENUITEM "&9",                          ID_PPU_FRAMESKIP_9
        END
        POPUP "&Size"
        BEGIN
            MENUITEM "&1X",                         ID_PPU_SIZE_1X
            MENUITEM "&2X",                         ID_PPU_SIZE_2X
            MENUITEM "&3X",                         ID_PPU_SIZE_3X
            MENUITEM "&4X",                         ID_PPU_SIZE_4X
            MENUITEM SEPARATOR
            MENUITEM "&Fix Aspect",                 ID_PPU_SIZE_ASPECT
        END
        POPUP "&Mode"
        BEGIN
            MENUITEM "&NTSC",                       ID_PPU_MODE_NTSC
            MENUITEM "&PAL",                        ID_PPU_MODE_PAL
            MENUITEM "&Hybrid",                     ID_PPU_MODE_DENDY
        END
        MENUITEM "P&alette...",                 ID_PPU_PALETTE
        POPUP "Sl&owdown"
        BEGIN
            MENUITEM "&Enabled\tTab",               ID_PPU_SLOWDOWN_ENABLED
            MENUITEM SEPARATOR
            MENUITEM "1/&2 (50%)",                  ID_PPU_SLOWDOWN_2
            MENUITEM "1/&3 (33%)",                  ID_PPU_SLOWDOWN_3
            MENUITEM "1/&4 (25%)",                  ID_PPU_SLOWDOWN_4
            MENUITEM "1/&5 (20%)",                  ID_PPU_SLOWDOWN_5
            MENUITEM "1/&10 (10%)",                 ID_PPU_SLOWDOWN_10
            MENUITEM "1/2&0 (5%)",                  ID_PPU_SLOWDOWN_20
        END
        MENUITEM "F&ullscreen\tAlt+Enter",      ID_PPU_FULLSCREEN
        MENUITEM "S&canlines",                  ID_PPU_SCANLINES
    END
    POPUP "&Sound"
    BEGIN
        MENUITEM "&Enabled\tCtrl+S",            ID_SOUND_ENABLED
        MENUITEM "&Volume",                     ID_SOUND_VOLUME
    END
    POPUP "&Input"
    BEGIN
        MENUITEM "&Setup...",                   ID_INPUT_SETUP
    END
    POPUP "&Debug"
    BEGIN
        MENUITEM "Dis&assembly\tCtrl+F1",       ID_DEBUG_CPU
        MENUITEM "&Video\tCtrl+F2",             ID_DEBUG_PPU
        MENUITEM SEPARATOR
        MENUITEM "S&tatus Window",              ID_DEBUG_STATWND
    END
    MENUITEM "&Game",                       ID_GAME, GRAYED
    POPUP "&Misc"
    BEGIN
        MENUITEM "S&tart AVI Capture...",       ID_MISC_STARTAVICAPTURE
        MENUITEM "St&op AVI Capture",           ID_MISC_STOPAVICAPTURE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "P&lay Movie...",              ID_MISC_PLAYMOVIE
        MENUITEM "&Record Movie...",            ID_MISC_RECORDMOVIE
        MENUITEM "Sto&p Movie",                 ID_MISC_STOPMOVIE, GRAYED
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About...",                   ID_HELP_ABOUT
    END
END


